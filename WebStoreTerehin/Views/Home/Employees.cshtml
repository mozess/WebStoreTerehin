@model IEnumerable<WebStoreTerehin.Models.Employee>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Employees</title>
</head>
<body>
    <h3>Сотрудники</h3>
    <hr />
    <table>
        <tr>
            <th>Фамилия</th>
            <th>Имя</th>
            <th>Отчество</th>
            <th>Возраст</th>
        </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model)
            { 
            <tr>
                <td>@employee.Name</td>
                <td>@employee.Surname</td>
                <td>@employee.Patronymic</td>
                <td>@employee.Age</td>
            </tr>
            }
        </tbody>
    </table>
    <hr />
    <a href="~/">На главную</a>
</body>
</html>
